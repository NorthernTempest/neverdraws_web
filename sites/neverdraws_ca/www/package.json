{
  "name": "www",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-compile": "tsc && node serve",
    "dev": "node .\\node_modules\\nodemon\\bin\\nodemon.js -e ts --exec \"npm run dev-compile\"",
    "build:client": "tsc && vite build --outDir dist/client --ssrManifest",
    "build:server": "tsc && vite build --outDir dist/server --ssr --ssrManifest",
    "build": "npm run build:client && npm run build:server"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router": "^5.1.20",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
